      <Query>
        <DataSourceName>NBA</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select [Measures].[Turnover - Playoffs] on 0,
Filter(crossjoin([Players].[Player ID].children,[Players].[Firstname].children,[Players].[Lastname].children),
[Measures].[Turnover - Playoffs]&gt;300) on 1
from [Project]
where [Players].[Position].[G]</CommandText>
